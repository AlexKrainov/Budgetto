<table class="datatables-demo table table-striped table-bordered"
	   id="table"
	   data-searching="true"
	   data-paging="false">
	<thead>
		<tr class="text-center">
			<th v-for="column in template.columns" v-html="column.name" v-show="column.isShow"></th>
		</tr>
	</thead>
	<tbody>
		<tr v-for="(row, rowIndex) in rows" class="text-right">
			<td v-for="(cell, cellIndex) in row"
				v-show="cell.isShow"
				v-html="getCellValue(cell)"
				v-on:click="clickCell(rowIndex, cellIndex)"
				class="cell-hover">
			</td>
		</tr>
	</tbody>
	<tfoot>
		<tr>
			<td v-for="(cell, cellIndex) in footerRow"
				v-show="cell.isShow"
				v-html="getCellValue(cell)"
				v-on:click="clickFooterCell(cellIndex)"
				class="cell-hover">
			</td>
		</tr>
	</tfoot>
</table>
@await Html.PartialAsync("_TimeLineDialog")